package ch4;

public class ForTest {

	public static void main(String[] args) {
		int total = 0;
		for(int i = 1; i <= 10; i++) {
			total = total + i;
			System.out.println("i=" + i + "일 때, total=" + total);
		}
		System.out.println(total);
		
		int total2 = 0;
		int i2 = 101;
		while( i2 <= 100 ) {
			total2 = total2 +i2;
			System.out.println("i2=" + i2 + "일 때, total2=" + total2);
			i2++;
		}
	}

}
